{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "my-lib",
    "baseUrl": ".",
    "paths": {
      "my-lib/jsx-runtime": ["./jsx/index.ts"],
      "my-lib/jsx-dev-runtime": ["./jsx/index.ts"]
    }
  },
  "include": ["src", "jsx"]
}
